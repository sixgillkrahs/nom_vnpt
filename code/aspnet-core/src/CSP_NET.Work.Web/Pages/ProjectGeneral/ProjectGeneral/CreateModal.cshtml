@page
@using Microsoft.AspNetCore.Mvc.Localization
@using CTIN.Csp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal;
@using CSP_NET.Work.Localization
@inject IHtmlLocalizer<WorkResource> L
@model CSP_NET.Work.Web.Pages.ProjectGeneral.ProjectGeneral.CreateModalModel
@{
    Layout = null;
}
<abp-dynamic-form abp-model="ViewModel" data-ajaxForm="true" asp-page="CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["CreateProjectGeneral"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>
